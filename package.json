{
  "name": "lunch-mator",
  "version": "1.0.0",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "build": "rm -rf target/karma && tsc -p ./tsconfig-test.json && rsync -avm --include='*.html' -f 'hide,! */' app/assets/javascripts target/karma",
    "pretest": "npm run build",
    "test": "karma start --single-run --browsers PhantomJS",
    "lite": "lite-server",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/common": "~2.4.3",
    "@angular/compiler": "~2.4.3",
    "@angular/core": "~2.4.3",
    "@angular/forms": "~2.4.3",
    "@angular/http": "~2.4.3",
    "@angular/platform-browser": "~2.4.3",
    "@angular/platform-browser-dynamic": "~2.4.3",
    "@angular/platform-server": "~2.4.3",
    "@angular/router": "~3.4.3",
    "angular-in-memory-web-api": "0.1.1",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "ng2-datetime": "^1.2.1",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-beta.12",
    "systemjs": "0.19.27",
    "zone.js": "0.6.25"
  },
  "devDependencies": {
    "concurrently": "^2.2.0",
    "lite-server": "^2.2.2",
    "typescript": "^2.0.2",
    "jasmine-core": "2.4.1",
    "phantomjs-prebuilt": "^2.1.4",
    "ng2lint": "0.0.6",
    "karma": "0.13.19",
    "karma-chrome-launcher": "0.2.2",
    "karma-coverage": "0.5.3",
    "karma-jasmine": "0.3.6",
    "karma-junit-reporter": "0.2.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "typings": "^1.3.2"
  }
}
