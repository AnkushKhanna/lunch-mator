# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

#Restaurant
GET         /rest/restaurants        controllers.RestaurantController.listRestaurants()
POST        /rest/restaurants        controllers.RestaurantController.createRestaurant()
# Map static resources from the /public folder to the /assets URL path
GET         /assets/*file            controllers.Assets.versioned(path="/public", file: Asset)


# Make HTML5 links work
GET         /                        controllers.Application.index(any: String ?= "none")
